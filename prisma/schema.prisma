generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Visit {
  id                String   @id @default(cuid())
  createdAt         DateTime @default(now())

  ip                String
  clientIpReported  String?
  city              String?
  region            String?
  country           String?

  userAgent         String?
  platform          String?
  device            String?
  latitude          Float?
  longitude         Float?

  timezone          String?
  asn               String?
  org               String?
  network           String?

  geoRaw            Json?
  @@map("visits")
}